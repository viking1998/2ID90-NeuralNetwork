      <BorderPane>
         <center>
            <ScrollPane fitToHeight="true" fitToWidth="true" hbarPolicy="NEVER" BorderPane.alignment="TOP_LEFT">
               <content>
                  <VBox fx:id="activationVBox" spacing="10.0" />
               </content>
            </ScrollPane>
         </center>
         <left>
            <VBox prefHeight="200.0" prefWidth="100.0" spacing="10.0" BorderPane.alignment="CENTER">
               <children>
                  <Label text="Batches " />
                  <ChoiceBox fx:id="batchesChoiceBox" prefWidth="150.0" />
                  <Label text="Activations" />
                  <ComboBox fx:id="activationsComboBox" prefWidth="150.0" />
               </children>
               <BorderPane.margin>
                  <Insets />
               </BorderPane.margin>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </VBox>
         </left>
      </BorderPane>
